{{!< default}}
<div class="gh-page">
    <div class="projects-header-wrapper">
        <a aria-label="projects" id="projects" class="projects-header-title">
          Projects
        </a>
      </div>
    <div class="gh-container">

        <div class="gh-postfeed">
        {{#foreach posts}}
            {{#has tag="project"}}
                {{> "card" page=../pagination.page }} {{!-- partials/card.hbs --}}
            {{/has}}
        {{/foreach}}
        </div>

        {{!-- If JS isn't enabled, fallback to standard pagination --}}
        <noscript>
            <div class="gh-pagination">
                {{pagination}}
            </div>
        </noscript>
    </div>
</div>
